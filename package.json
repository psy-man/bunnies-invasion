{
  "name": "bunnies-apocalypse",
  "version": "0.0.1",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:dll": "rimraf dll",
    "dll": "npm run clean:dll && webpack --config config/webpack.dll.js",
    "postinstall": "npm run dll",
    "serve": "webpack-dev-server --inline --colors",
    "start": "npm run dll && npm run serve",
    "build": "export NODE_ENV=prod && npm run clean:dist && webpack"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "@types/pixi.js": "^4.4.2",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.1.6",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "pathfinding": "^0.4.18",
    "pixi.js": "^4.4.4"
  }
}
